{
    "name": "Timetable on myRMIT",
    "version": "1.0",
    "manifest_version": 2,
 "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://my.rmit.edu.au/portal/*"],
      "js":["RMIT.js"],
      "run_at": "document_start"
    }
  ],
   "permissions":[
   "http://*/*", "https://*/*"
  ],
    "description": "Adding the timetable back to myRMIT"
}
